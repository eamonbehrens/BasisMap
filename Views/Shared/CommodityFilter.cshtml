@model IEnumerable<string>

<li class="dropdown">
    <a asp-area="" asp-controller="Store" asp-action="Index" class="dropdown-toggle" data-toggle="dropdown">Store <b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach (var commodityDescription in Model)
        {
            <li>
                <a asp-area="" asp-controller="Home" asp-action="Browse" 
                   asp-route-Genre="@commodityDescription">@commodityDescription</a>
            </li>
        }
        <li class="divider"></li>
        <li>
            <a asp-controller="Home" asp-action="Index">More...</a>
        </li>
    </ul>
</li>





