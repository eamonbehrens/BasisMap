@*@using System.Threading.Tasks*@
@*@using BasisMap.Models*@
@model IEnumerable<BasisMap.Models.Commodity>
@{
    ViewData["Title"] = "Characters";
}
<h1>Characters</h1>
<div class="row">
    <div class="col-md-12">
        <table>
            <tr>
                <th>Name</th>
            </tr>
            @foreach (var character in Model)
            {
                <tr>
                    <td>@character.Code</td>
                </tr>
            }
            <tr>
            </tr>
        </table>
    </div>
</div>
